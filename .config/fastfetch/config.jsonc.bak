{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
      "source": ".config/fastfetch/ascii/pikachu.txt",
      "type": "file",
      "color": {
          "1": "yellow"
      }
  },
  "display": {
      "separator": "  "
  },
  "modules": [
      {
          "type": "custom",
          "format": "\u001b[1;93mHere's information on the running host machine..."
      },
      {
          "type": "custom",
          "format": "╭──────────────────── \u001b[1;31mHardware\u001b[0m ────────────────────┐"
      },
      {
          "type": "cpu",
          "key": "\u001b[0m│ \u001b[1;93m CPU"
      },
      {
          "type": "gpu",
          "key": "\u001b[0m│ \u001b[1;93m󰘚 GPU"
      },
      {
          "type": "memory",
          "key": "\u001b[0m│ \u001b[1;93m RAM"
      },
      {
          "type": "custom",
          "format": "╰──────────────────────────────────────────────────┘"
      },
      {
          "type": "title",
          "key": "  \u001b[1;93m",
          "format": "{1}@{2}"
      },
      {
          "type": "custom",
          "format": "╭──────────────────── \u001b[1;31mSoftware\u001b[0m ────────────────────┐"
      },
      {
          "type": "os",
          "key": "\u001b[0m│ \u001b[1;93m System"
      },
      {
          "type": "kernel",
          "key": "\u001b[0m│ \u001b[1;93m Kernel"
      },
      {
          "type": "wm",
          "key": "\u001b[0m│ \u001b[1;93m Desktop"
      },
      {
          "type": "terminal",
          "key": "\u001b[0m│ \u001b[1;93m Terminal"
      },
      {
          "type": "shell",
          "key": "\u001b[0m│ \u001b[1;93m \u001b[1;30mCurrent Shell"
      },
      {
          "type": "packages",
          "key": "\u001b[0m│ \u001b[1;93m󰏖 Packages"
      },
      {
          "type": "battery",
          "key": "\u001b[0m│ \u001b[1;93m Battery"
      },
      {
          "type": "uptime",
          "key": "\u001b[0m│ \u001b[1;93m󱑀 Uptime"
      },
      {
          "type": "custom",
          "format": "╰──────────────────────────────────────────────────┘"
      },
      {
          "type": "colors",
          "paddingLeft": 2,
          "symbol": "circle"
      },
      {
          "type": "custom",
          "format": "\u001b[1mHave a lovely day! Remember to stay positive and hydrated ^_^"
      }
  ]
}
